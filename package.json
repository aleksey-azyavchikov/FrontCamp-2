{
  "name": "front-camp",
  "version": "1.0.0",
  "scripts": {
    "ui:build:dev": "webpack --config configs/webpack/dev.config.js",
    "ui:build:prod": "webpack --config configs/webpack/prod.config.js",
    "ui:watch:dev": "npm run ui:build:dev --watch",
    "ui:watch:prod": "npm run ui:build:prod --watch",
    "ui:server:dev": "webpack-dev-server --config configs/webpack/dev.config.js",
    "ui:server:prod": "webpack-dev-server --config configs/webpack/prod.config.js --open",
    "db:setup": "start .\\scripts\\setup-and-run.bat",
    "db:run:server": "start cmd /c .\\scripts\\run-db-server.cmd",
    "db:run:client": "start cmd /c .\\scripts\\run-db-client.cmd",
    "db:run:both": "npm run db:run:server && npm run db:run:client",
    "db:export": "start cmd /c .\\scripts\\export-collections.cmd",
    "db:import": "start .\\scripts\\import-db.bat",
    "server:run": "nodemon ./server/bin/www"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^8.1.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-dynamic-import-webpack": "^1.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.16.0",
    "babel-preset-env": "^1.6.1",
    "bootstrap": "^4.0.0-beta.2",
    "css-loader": "^0.28.7",
    "eslint": "^4.14.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "jquery": "^3.2.1",
    "node-sass": "^4.7.2",
    "popper.js": "^1.12.9",
    "postcss-loader": "^2.0.9",
    "precss": "^2.0.0",
    "rxjs": "^5.5.6",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.1.5",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7",
    "webpack-merge": "^4.1.1",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "debug": "^3.1.0",
    "express": "^4.16.2",
    "jade": "^1.11.0",
    "morgan": "^1.9.0",
    "serve-favicon": "^2.4.5",
    "mongoose": "^5.0.1",
    "nodemon": "^1.14.11"
  }
}
