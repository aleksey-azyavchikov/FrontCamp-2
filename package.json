{
    "name": "front-camp",
    "version": "1.0.0",
    "scripts": {
        "ui:build:dev": "SET NODE_ENV=development && webpack --config configs/webpack/dev.config.js",
        "ui:build:prod": "SET NODE_ENV=production && webpack --config configs/webpack/prod.config.js",
        "ui:watch:dev": "npm run ui:build:dev --watch",
        "ui:watch:prod": "npm run ui:build:prod --watch",
        "ui:run:dev": "SET NODE_ENV=development && webpack-dev-server --config configs/webpack/dev.server.config.js",
        "ui:run:prod": "SET NODE_ENV=production && webpack-dev-server --config configs/webpack/prod.config.js --open",
        "db:setup": "start .\\scripts\\setup-and-run.bat",
        "db:run:server": "start cmd /c .\\scripts\\run-db-server.cmd",
        "db:run:client": "start cmd /c .\\scripts\\run-db-client.cmd",
        "db:run:both": "npm run db:run:server && npm run db:run:client",
        "db:export": "start cmd /c .\\scripts\\export-collections.cmd",
        "db:import": "start .\\scripts\\import-db.bat",
        "server:run:dev": "SET NODE_ENV=development && npm run db:run:server && npm run nodemon:server",
        "server:run:prod": "SET NODE_ENV=production && npm run nodemon:server",
        "full:run:dev": "npm run server:run:dev && npm run ui:run:dev",
        "full:run:prod": "npm run server:run:prod && npm run ui:run:prod",
        "app:run:dev": "SET NODE_ENV=development && npm run ui:build:dev && node ./server/bin/www",
        "app:run:prod": "SET NODE_ENV=production && npm run ui:build:prod && node ./server/bin/www",
        "nodemon:server": "nodemon ./server/bin/www",
        "node:server": "node ./server/bin/www",
        "test": "jest",
        "test:watch": "jest --watch",
        "server:run:debug": "nodemon --debug ./server/bin/www"
    },
    "devDependencies": {
        "babel-cli": "^6.18.0",
        "babel-eslint": "^8.1.2",
        "babel-jest": "^22.4.1",
        "babel-loader": "^7.1.2",
        "babel-plugin-dynamic-import-webpack": "^1.0.2",
        "babel-plugin-react-html-attrs": "^2.1.0",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-polyfill": "^6.16.0",
        "babel-preset-env": "^1.6.1",
        "babel-preset-react": "^6.24.1",
        "body-parser": "^1.18.2",
        "bootstrap": "^4.0.0-beta.2",
        "cookie-parser": "^1.4.3",
        "css-hot-loader": "^1.3.7",
        "css-loader": "^0.28.7",
        "debug": "^3.1.0",
        "enzyme": "^3.3.0",
        "eslint": "^4.14.0",
        "eslint-plugin-react": "^7.6.1",
        "express": "^4.16.2",
        "extract-text-webpack-plugin": "^3.0.2",
        "file-loader": "^1.1.5",
        "html-loader": "^0.5.1",
        "html-webpack-plugin": "^2.30.1",
        "jade": "^1.11.0",
        "jest": "^22.4.2",
        "jest-css-modules": "^1.1.0",
        "jquery": "^3.2.1",
        "mongoose": "^5.0.1",
        "morgan": "^1.9.0",
        "node-sass": "^4.7.2",
        "nodemon": "^1.14.11",
        "popper.js": "^1.12.9",
        "postcss-loader": "^2.0.9",
        "precss": "^2.0.0",
        "prop-types": "^15.6.1",
        "react": "^16.2.0",
        "react-addons-test-utils": "^15.6.2",
        "react-dom": "^16.2.0",
        "react-hot-loader": "^3.1.3",
        "react-redux": "^5.0.7",
        "react-router-dom": "^4.2.2",
        "react-test-renderer": "^16.2.0",
        "redux": "^3.7.2",
        "redux-act": "^1.7.3",
        "redux-logger": "^3.0.6",
        "redux-thunk": "^2.2.0",
        "rxjs": "^5.5.6",
        "sass-loader": "^6.0.6",
        "serve-favicon": "^2.4.5",
        "style-loader": "^0.19.0",
        "uglifyjs-webpack-plugin": "^1.1.5",
        "url-loader": "^0.6.2",
        "webpack": "^3.10.0",
        "webpack-dev-server": "^2.9.7",
        "webpack-merge": "^4.1.1"
    },
    "dependencies": {
        "connect-mongo": "^2.0.1",
        "express-session": "^1.15.6",
        "lodash": "^4.17.5",
        "react-highlight-words": "^0.11.0",
        "redux-loop": "^4.2.4",
        "webpack-dev-middleware": "^3.0.1"
    },
    "jest": {
        "transform": {
            ".*": "<rootDir>/node_modules/jest-css-modules"
        }
    }
}
